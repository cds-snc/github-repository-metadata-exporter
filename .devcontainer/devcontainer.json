{
	"name": "github-repository-metadata-exporter",
	"image": "mcr.microsoft.com/vscode/devcontainers/base:buster",
	"features": {
		"node": {
			"version": "lts"
		}
	},
	"containerEnv": {
		"SHELL": "/bin/zsh"
	},
	"extensions": [
		"redhat.vscode-yaml",
		"github.copilot"
	],
	"remoteUser": "vscode",
	"postCreateCommand": ".devcontainer/postCreateCommand.sh"
}